    block scroll_nav_change
      script(type="text/javascript").
        window.onscroll = function() {
          if(document.body.scrollTop + document.documentElement.scrollTop > 20){
            document.querySelector('#nav').classList.add("shrink");
            document.querySelector('.navbar-brand img').src = "/img/logo/vmfive--logo-standard.svg";
          }else{
            document.querySelector('#nav').classList.remove("shrink");
            document.querySelector('.navbar-brand img').src = "/img/logo/vmfive--logo.svg";
          }
        }
